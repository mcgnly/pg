{
  "extends": ["airbnb", "airbnb/hooks", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension" : [0],
    "no-use-before-define" : [0],
    "react/jsx-curly-brace-presence" : [0],
    "react/no-array-index-key" : [0],
    "react/prop-types" : [0],
  },
}
